<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:chat="clr-namespace:ChatBot.Chat"
					xmlns:login="clr-namespace:ChatBot.Login">
    <DataTemplate DataType="{x:Type chat:ChatViewModel}">
        <chat:ChatUserControl/>
    </DataTemplate>
	<DataTemplate DataType="{x:Type login:LoginViewModel}">
		<login:LoginUserControl/>
	</DataTemplate>
</ResourceDictionary>
